{
  "name": "doubter",
  "version": "2.1.0",
  "description": "No-hassle runtime validation and transformation library.",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./validators": {
      "types": "./validators/index.d.ts",
      "import": "./validators/index.mjs",
      "require": "./validators/index.js"
    },
    "./validators/arrays": {
      "types": "./validators/arrays.d.ts",
      "import": "./validators/arrays.mjs",
      "require": "./validators/arrays.js"
    },
    "./validators/numbers": {
      "types": "./validators/numbers.d.ts",
      "import": "./validators/numbers.mjs",
      "require": "./validators/numbers.js"
    },
    "./validators/sets": {
      "types": "./validators/sets.d.ts",
      "import": "./validators/sets.mjs",
      "require": "./validators/sets.js"
    },
    "./validators/strings": {
      "types": "./validators/strings.d.ts",
      "import": "./validators/strings.mjs",
      "require": "./validators/strings.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup --config && cp package.json README.md LICENSE.txt lib/",
    "clean": "rimraf ./lib ./docs ./coverage",
    "test": "jest",
    "test:definitions": "ln -nsf ../lib ./node_modules/doubter && tsd --files './src/test/**/*.test-d.ts'",
    "perf": "toofast",
    "docs": "typedoc ./src/main/index.typedoc.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/doubter.git"
  },
  "keywords": [
    "typings",
    "validate",
    "parse",
    "runtime",
    "union",
    "lazy"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/doubter/issues"
  },
  "homepage": "https://github.com/smikhalevski/doubter#readme",
  "devDependencies": {
    "@badrap/valita": "^0.2.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^29.5.1",
    "@types/qs": "^6.9.7",
    "ajv": "^8.12.0",
    "jest": "^29.5.0",
    "myzod": "^1.10.0",
    "prettier": "^2.8.8",
    "qs": "^6.11.1",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.6",
    "rollup-plugin-dts": "^5.3.0",
    "toofast": "^2.0.0",
    "ts-jest": "^29.1.0",
    "tsd": "^0.28.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4",
    "zod": "^3.21.4"
  }
}
