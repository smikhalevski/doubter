{
  "compilerOptions": {
    "importHelpers": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "strict": true,
    "target": "ES5",
    "module": "ES6",
    "baseUrl": ".",
    "paths": {
      "doubter": [
        "./src/main"
      ],
      "doubter/*": [
        "./src/main/*"
      ]
    }
  },
  "include": [
    "./src/main/**/*"
  ]
}
